{
  "name": "employee-bonus-api",
  "version": "0.6.0",
  "description": "An AWS Lambda-based web API that receives employee bonus data.",
  "author": "Tramaine L. Gillus",
  "repository": {
    "type": "git",
    "url": "git@github.com:tgillus/employee-bonus-api.git"
  },
  "license": "ISC",
  "main": "build/server.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "build:artifact": "npx gulp build",
    "build:artifact:clean": "npx gulp clean",
    "clean": "rimraf ./build",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepare": "husky install",
    "start": "npm run build && NODE_OPTIONS=--enable-source-maps node build/server.js",
    "start:dev": "nodemon",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "watch": "tsc -c"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.27.0",
    "@types/aws-lambda": "^8.10.83",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/lambda-log": "^2.2.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.3.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.30.0",
    "execa": "^5.1.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-zip": "^5.1.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "serverless": "^2.55.0",
    "serverless-offline": "^8.0.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.27.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lambda-log": "^3.0.0",
    "morgan": "^1.10.0",
    "serverless-http": "^2.7.0",
    "winston": "^3.3.3"
  }
}
